{
    "manifest_version": 3,
   
    "name": "Go Back Last Tab History",
    "version": "1.0.0",
    "icons": { "512": "icon.png"},
    "description": "Go Back and Forward to Last Viewed Tabs In Current Window",
    "permissions": [
       "storage"
    ],
    "background": {
      "service_worker": "backgroundTabs.js" 
    },
    "commands": {
      "cycle-back": {
        "suggested_key": {
          "default": "Alt+Shift+S"
        },
        "description": "Cycle Tab"
      }, 
      "cycle-forward": {
        "suggested_key": {
          "default": "Alt+Shift+F"
        },
        "description": "Cycle Tab"
      }
    }
  }